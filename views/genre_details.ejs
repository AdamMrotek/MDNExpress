<!DOCTYPE html>
<html>
  </body> 
    <%- include('./partials/head') %>
    <div class="container-fluid">
      <div class="row">
        <%- include('./partials/nav') %>
          <div class="col-sm-10">
<h1>Genre: <%= genre.genre.name %> </h1>
<h4>Books</h4>
  <% if(genre.books.length >0){%>
    <% genre.books.forEach(book =>  { %> 
      <dl>
        
            <a href="<%=book.url%>"><%=book.title %> </a>
            <dd><%=book.summary%></dd>  
        
      </dl>
      <%})%> 
  <% }else{ %>  
    <p>No books to display</p>
  <% } %> 
</div>
</div>
</div> 
</body>
</html>